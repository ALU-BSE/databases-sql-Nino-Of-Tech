db.transactions.aggregate([
  {$group: {_id: "pK7FAnN9u5GZlC5u+nRo3Rfn7Vw", count: {$sum: 1}}},
  {$sort: {count: -1}},
  {$limit: 5},
  {$lookup: {from: "products", localField: "_id", foreignField: "_id", as: "productInfo"}},
  {$unwind: "$productInfo"},
  {$project: {productName: "$productInfo.name", count: 1}}
])
